public class Caso7{

  public static void main(){

    int lavoro = 0;
    int i;
    int j = 1;
    int V[] = { 22, 5, 7, 11, 2, 33, 9};   
        
    for(i = 0; i < 7; i++){
      for(j = i + 1; j < 7; j++){
        if(V[i] > V[j]){
           lavoro = V[i];
           V[i] = V[j];
           V[j] = lavoro;
        }
      }
    }  
 }
}
;
